<div class="row">
  <div>
    <div class="col-md-3">
      <div class="well sidebar-nav">
        <strong>Pages</strong>
        <ul class="list-group">
          {{#each page in model}}
            <li class="list-group-item">{{#link-to 'pages.page' this}}{{page.title}}{{/link-to}}</li>
          {{/each}}
        </ul>
      </div>
    </div>
    <div class="col-md-9">
      {{outlet}}
    </div>
  </div>
</div>
